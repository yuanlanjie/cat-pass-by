void setup(){
  float ww = 0;
  float hh = 20;
  size(1800,600);
  background(0);
  sentences = new String[9];
  sentences[0] = "seven times have i despised my soul";
  sentences[1] = "kahlil gibran";
  sentences[2] = "the first time when i saw her being meek that she might attain height";
  sentences[3] = "the second time when i saw her limping before the crippled";
  sentences[4] = "the third time when she was given to choose between the hard and the easy, and she chose the easy";
  sentences[5] = "the fourth time when she committed a wrong, and comforted herself that others also commit wrong";
  sentences[6] = "the fifth time when she forbore for weakness, and attributed her patience to strength.";
  sentences[7] = "the sixth time when she despised the ugliness of a face, and knew not that it was one of her own masks";
  sentences[8] = "and the seventh time when she sang a song of praise, and deemed it a virtue";
}

void draw(){
  //background(0);
  //float ww = 0;
  //float hh = 20;
  for(int i = 0; i< 9; i++){
    //float hh = 20*i;
    ww =20;
    hh += 50;
    for(int j = 0; j< sentences[i].length(); j++){
      char cc = sentences[i].charAt(j);
    switch(cc){
      case 'a':
        shapeA();
        //ww = ww +20;
        break;
      case 'b':
        shapeB();
        //ww = ww+20;
        break;
      case 'c':
        shapeC();
        //ww +=20;
        break;
      case 'd':
        shapeD();
        //ww +=20;
        break;
      case 'e':
        shapeE();
        //ww +=20;
        break;
      case 'f':
