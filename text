        //ww +=20;
        break;
      case 'p':
        shapeP();
        //ww +=20;
        break;
      case 'q':
        shapeQ();
        //ww +=20;
        break;
      case 'r':
        shapeR();
        //ww +=20;
        break;
      case 's':
        shapeS();
        //ww +=20;
        break;
      case 't':
        shapeT();
        //ww +=20;
        break;
      case 'u':
        shapeU();
        //ww +=20;
        break;
      case 'v':
        shapeV();
        //ww +=20;
        break;
      case 'w':
        shapeW();
        //ww +=20;
        break;
      case 'x':
        shapeX();
        //ww +=20;
        break;
      case 'y':
        shapeY();
        //ww +=20;
        break;
      case 'z':
        shapeZ();
        //ww +=20;
        break;
    }
    ww +=25;
    int cl = int(cc);
    println(cl);
    frameRate(1);
    //hh = 20*i;
  }
    //ww +=20;
    
  }
  
  
  
  
  
  
  
