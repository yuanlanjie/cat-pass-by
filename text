        //ww +=20;
        break;
      case 'i':
        shapeI();
        //ww +=20;
        break;
      case 'j':
        shapeJ();
        //ww +=20;
        break;
      case 'k':
        shapeK();
        break;
      case 'l':
        shapeL();
        //ww +=20;
        break;
